<Window x:Class="JobOrganizerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JobOrganizerWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="850">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="148*"/>
            <ColumnDefinition Width="369*"/>
        </Grid.ColumnDefinitions>
        <WebBrowser x:Name="myWebBrowser" HorizontalAlignment="Left" Height="449" VerticalAlignment="Top" Width="679" Margin="153,10,0,0" Grid.ColumnSpan="2" Source="{x:Null}"/>
        <Button x:Name="btnCreate" Content="Create Event" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="138" Height="26" Click="button_Click"/>
        <TextBox x:Name="txtCalendarLink" HorizontalAlignment="Left" Height="24" Margin="10,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="138"/>

    </Grid>
</Window>
